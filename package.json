{
  "name": "sample",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "gcp-build": "yarn build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "STAGE=production node dist/src/main",
    "dev": "dotenv -e .local.env -- nest start --watch",
    "dev:debug": "dotenv -e .local.env -- nest start --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint --fix \"src/**/*.ts\"",
    "migrate:dev": "dotenv -e .local.env -- prisma migrate dev --preview-feature",
    "model-generate": "prisma generate",
    "migrate:dev:reset": "dotenv -e .local.env -- prisma migrate reset --preview-feature",
    "studio:dev": "dotenv -e .local.env -- prisma studio",
    "seed:dev": "dotenv -e .local.env -- ts-node prisma/seed.ts",
    "seed": "ts-node prisma/seed.ts",
    "migrate:test": "dotenv -e .test.env -- prisma migrate reset --force --preview-feature",
    "migrate:prd": "prisma migrate deploy --preview-feature",
    "test:unit": "dotenv -e .test.env -- jest",
    "test:watch": "dotenv -e .test.env -- jest --verbose --watchAll",
    "test:integration": "jest -c ./jest.integration.config.js --runInBand",
    "prepare": "husky install"
  },
  "dependencies": {
    "@nestjs/common": "^7.5.1",
    "@nestjs/core": "^7.5.1",
    "@nestjs/platform-express": "^7.5.1",
    "@nestjs/swagger": "^4.7.12",
    "@prisma/client": "^3.9.1",
    "@types/nodemailer": "^6.4.4",
    "class-transformer": "^0.3.2",
    "class-validator": "^0.13.1",
    "csv": "^5.3.2",
    "dotenv": "^8.2.0",
    "nodemailer": "^6.7.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "swagger-ui-express": "^4.1.6",
    "uuidv4": "^6.2.6"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.1",
    "@nestjs/schematics": "^7.1.3",
    "@nestjs/testing": "^7.5.1",
    "@types/express": "^4.17.8",
    "@types/faker": "^5.1.6",
    "@types/jest": "^26.0.15",
    "@types/node": "^15.14.0",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.4",
    "@types/validator": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-prettier": "^3.1.4",
    "faker": "^5.1.0",
    "husky": "^8.0.1",
    "jest": "^26.6.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.1.2",
    "prisma": "^3.9.1",
    "supertest": "^6.0.0",
    "ts-jest": "^26.4.3",
    "ts-loader": "^8.0.8",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.6.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix \"src/**/*.ts\"",
      "prettier --write \"src/**/*.ts\""
    ]
  }
}
